<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ShiftableEditText">

        <attr name="android:text"/>
        <attr name="android:textSize"/>
        <attr name="android:textColor"/>
        <attr name="android:hint"/>
        <attr name="baseColor" format="color"/>
        <attr name="highlightColor" format="color"/>
        <attr name="iconColor" format="color"/>
        <attr name="iconKey" format="string"/>
        <attr name="labelColor" format="color"/>
        <attr name="mode" format="enum">
            <enum name="edit" value="0"/>
            <enum name="marquee" value="1"/>
        </attr>
        <attr name="android:inputType"/>
        <attr name="shiftableEditTextStyle" format="reference"/>

    </declare-styleable>

</resources>
